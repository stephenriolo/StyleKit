{
    "@mainFont": "HelveticaNeue-Bold:20.0",
    "@secondaryFont": "HelveticaNeue-Medium:13.0",
    "@primaryColor": "#cd0073",
    "@secondaryColor": "#8000c6",
    "@transparencyValue": 90,
    "@textFieldStyle": {
        "backgroundColor": "#000000"
    },
    "@buttonStyle": {
        "font": "@secondaryFont",
        "titleColor:normal": "@primaryColor",
        "titleColor:highlighted": "@secondaryColor",
        "backgroundColor": "#555"
    },
    "@labelStyle": {
        "font": "@mainFont",
        "color": "@primaryColor"
    },
    "@secondLabelStyle": {
        "font": "@secondaryFont",
        "color": "@secondaryColor"
    },
    "StyleKitDemo.SKNavigationBar": {
        "tintColor": "@secondaryColor",
        "titleTextAttributes": {
            "NSColor": "@primaryColor",
            "NSFont": "@mainFont"
        }
    },
    "UITableView": {
        "UITableViewCell": {
            "UILabel": {
                "font": "@secondaryFont",
                "color": "#000"
            }
        }
    },
    "UITableViewCell": {
        "UILabel": {
            "font": "@secondaryFont",
            "color": "#000"
        }
    },
    "StyleKitDemo.SKButton": {
        "backgroundColor:normal": "@primaryColor",
        "backgroundColor:highlighted": "@secondaryColor",
        "titleColor:normal": "#fff",
        "titleColor:highlighted": "#fff",
        "font": "HelveticaNeue-Light:13.0",
        "transparency": "@transparencyValue",
        
    },
    "UIButton": {
        "mixins":["@buttonStyle"]
    },
    "StyleKitDemo.SKLabel": {
        "font": "HelveticaNeue-Light:17.0",
        "color": "@primaryColor"
    },
    "UILabel": {
        "font": "HelveticaNeue-Bold:15.0",
        "color": "#8000c6"
    },
    "UIScrollView": {
        "UILabel": {
            "font": "HelveticaNeue-Light:15.0",
            "color": "#000"
        },
        "StyleKitDemo.SKView": {
            "StyleKitDemo.SKLabel": {
                "font": "HelveticaNeue-Bold:15.0",
                "color": "@primaryColor",
                "padding": [0,30,0,0],
                "backgroundColor": "#eee"
            }
        }
    },
    "UITextField": {
        "backgroundColor": "#FFF",
        "UILabel": {
            "mixins":["@labelStyle"]
        }
    },
    "StyleKitDemo.SKTextField": {
        "font": "HelveticaNeue-Light:15.0",
        "textColor": "#000",
        "mixins": ["@textFieldStyle"],
        "UILabel": {
            "color": "@primaryColor",
            "mixins": ["@secondLabelStyle"]
        }
    },
    "UISegmentedControl": {
        "tintColor": "@primaryColor",
        "titleTextAttributes:normal": {
            "NSColor": "#999",
            "NSFont": "HelveticaNeue-Light:15.0"
        },
        "titleTextAttributes:selected": {
            "NSColor": "#000",
            "NSFont": "HelveticaNeue-Medium:15.0"
        }
    },
    "UIActivityIndicatorView": {
        "color": "@primaryColor"
    }
}
